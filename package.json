{
	"name": "starteacher",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "npm stop && pm2 start ecosystem.config.js",
		"stop": "pm2 ls | grep -q starteacher; ([[ $? == 0 ]] && pm2 stop starteacher || echo \"OK\")",
		"build": "babel src --out-dir dist --verbose",
		"dev": "pm2-dev start ecosystem.config.js",
		"script": "node -r @babel/register -r @babel/polyfill -r dotenv/config",
		"docker:start": "docker-compose -f ./docker/dev.yml up",
		"docker:build": "docker-compose -f ./docker/dev.yml build",
		"docker:stop": "docker-compose -f ./docker/dev.yml down",
		"docker:restart": "npm run docker:stop && npm run docker:start",
		"jest:start": "jest --forceExit",
		"jest:test": "jest",
		"jest:file": "jest --no-color 2>output.txt --forceExit",
		"test": "NODE_ENV=test mocha \"src/**/*.test.js\""
	},
